<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.cpe.mobile" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <name>imajur</name>
    <description>
        Upload, vote and comment pictures with your friends !
    </description>
    <author email="5irc@cpe.fr" href="http://cpe.fr">
        Mathieu Degaine, Maxime Lesbros, Quentin Rosenstiehl, Boris LeDuff
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
